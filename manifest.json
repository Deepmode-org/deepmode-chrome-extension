{
  "name": "Deep Mode",
  "version": "0.1",
  "description": "Stay on task, online",
  "permissions": ["identity", "activeTab", "tabs", "storage"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "scripts": ["helpers/extractDomain.js", "content_scripts/textParamHandlers.js", "api/api.js", "background/background.js", "background/updateTask.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": [],
      "js": ["helpers/extractDomain.js", "content_scripts/textParamHandlers.js", "api/api.js", "content_scripts/content.js"],
      "run_at": "document_idle"
    },
    {
      "matches": ["<all_urls>"],
      "css": ["content_scripts/styles.css"],
      "js": ["api/api.js", "content_scripts/siteAugmentationHandlers.js", "content_scripts/youtube.js", "content_scripts/stackexchange.js", "content_scripts/quora.js", "content_scripts/articleParse.js", "content_scripts/twitter.js"],
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "images/icon_16.png",
      "32": "images/icon_32.png",
      "48": "images/icon_48.png",
      "128": "images/icon_128.png"
    }
  },
  "icons": {
    "16": "images/icon_16.png",
    "32": "images/icon_32.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "oauth2": {
    "client_id": "471502441995-vojtv32sakhrounlkk13np20cknr9pr2.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "key": "",
  "manifest_version": 2
}